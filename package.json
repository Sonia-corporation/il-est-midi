    "url": "https://github.com/C0ZEN"
  },
  "contributors": [
    {
      "email": "geoffrey.testelin@gmail.com",
      "name": "Geoffrey 'C0ZEN' Testelin",
      "url": "https://github.com/C0ZEN"
    }
  ],
  "description": "The Sonia Discord bot for the famous quote: il est midi",
  "homepage": "https://github.com/Sonia-corporation/il-est-midi-discord",
  "maintainers": [
    {
      "email": "geoffrey.testelin@gmail.com",
      "name": "Geoffrey 'C0ZEN' Testelin",
      "url": "https://github.com/C0ZEN"
    }
  ],
  "license": "MIT",
  "keywords": [
    "discord",
    "bot",
    "discord-bot",
    "sonia",
    "il-est-midi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sonia-corporation/il-est-midi-discord.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./scripts/commitizen.js"
    }
  },
  "private": false,
  "main": "./src/index.ts",
  "scripts": {
    "\n# BUILD SCRIPTS:": "",
    "build": "npm run clean:dist && concurrently \"parcel build ./src/index.ts --target node\" \"npm run build:environment:copy\" && npm run build:environment:rewrite-path",
    "build:environment:copy": "node scripts/build-environment-copy.js",
    "build:environment:rewrite-path": "node ./scripts/build-environment-rewrite-path.js",
    "start": "ts-node-dev --respawn --transpileOnly .",
    "start:prod": "npm run build && node ./dist/index.js",
    "tsc": "tsc",
    "\n# TEST SCRIPTS:": "",
    "test": "",
    "test:ci": "",
    "\n# LINT SCRIPTS:": "",
    "lint": "eslint --fix --ext .js,.ts --cache scripts/ src/",
    "lint:ci": "eslint --ext .js,.ts scripts/ src/",
    "\n# UTILS SCRIPTS:": "",
    "commitmsg": "cz-customizable-ghooks",
    "cz": "git add . && git-cz && git push-upstream",
    "semver": "npx semantic-release",
    "clean:dist": "rimraf dist"
  },
  "dependencies": {
    "discord.js": "12.0.2",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/app-root-path": "1.2.4",
    "@types/express": "4.17.3",
    "@types/fs-extra": "8.1.0",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.30",
    "@types/ws": "7.2.3",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "app-root-path": "3.0.0",
    "chalk": "3.0.0",
    "commitizen": "4.0.3",
    "concurrently": "5.1.0",
    "conventional-changelog-angular": "5.0.6",
    "cz-customizable": "6.2.0",
    "cz-customizable-ghooks": "1.5.0",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-recommended": "4.0.0",
    "eslint-plugin-import": "2.20.1",
    "fs-extra": "9.0.0",
    "husky": "4.2.3",
    "parcel-bundler": "1.12.4",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.4",
    "ts-node-dev": "1.0.0-pre.44",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  }
}
